---
import favIcon from '../../../images/favicon-16.png';
import { bundesligaTableService } from '../../services/bundesliga-table.service';
import { BundesligaTable } from '../../models/bundesliga-table';
import '../../components/bundesliga-table.page';
import '../../styles/page.css';
const bundesligaTable: BundesligaTable = await bundesligaTableService.getData();
const props: Record<string, string> = {
  'server-json-data': JSON.stringify(bundesligaTable)
};
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <link rel="icon" type="image/x-icon" href={favIcon} />
    <title>Bundesliga Tabelle - better-kickbase</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="description" content="better-kickbase-web Bundesliga Tabelle" />
  </head>
  <body>
    <bkb-bundesliga-table {...props} client:idle></bkb-bundesliga-table>
  </body>
</html>
